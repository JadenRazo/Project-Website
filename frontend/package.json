{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://jadenrazo.dev",
  "dependencies": {
    "@react-spring/web": "^9.7.3",
    "@react-three/drei": "^9.88.0",
    "@react-three/fiber": "^8.15.11",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.8",
    "@types/react": "^18.2.37",
    "@types/react-collapsible": "^2.3.1",
    "@types/react-dom": "^18.2.15",
    "@types/react-router-dom": "^5.3.3",
    "@types/recharts": "^1.8.29",
    "axios": "^1.6.2",
    "chart.js": "^4.4.0",
    "express": "^4.21.2",
    "framer-motion": "^10.16.5",
    "gsap": "^3.12.2",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-collapsible": "^2.10.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-intersection-observer": "^9.5.3",
    "react-router-dom": "^6.19.0",
    "react-scripts": "5.0.1",
    "recharts": "^2.15.1",
    "styled-components": "^6.1.15",
    "tailwindcss": "^3.3.5",
    "three": "^0.158.0",
    "typescript": "~4.9.5",
    "web-vitals": "^3.5.0",
    "zustand": "^4.5.7"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "serve": "serve -s build",
    "build:prod": "GENERATE_SOURCEMAP=false NODE_ENV=production NODE_OPTIONS='--max-old-space-size=3092' craco build",
    "build:analyze": "npm run build:prod && npx source-map-explorer 'build/static/js/*.js'",
    "watch": "npx nodemon --watch src --ext ts,tsx,js,jsx,json,css --exec \"npm run build\"",
    "start:prod": "http-server ./build -p 80 -a 0.0.0.0",
    "start:alt": "http-server ./build -p 8080 -a 0.0.0.0",
    "watch-and-serve": "npm run build && sudo -E npm run start:prod",
    "watch-dev": "concurrently \"npm run watch\" \"npm run start:alt\"",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "type-check": "tsc --noEmit",
    "lint": "eslint --ext .ts,.tsx src/",
    "lint:fix": "eslint --ext .ts,.tsx src/ --fix",
    "format": "prettier --write 'src/**/*.{ts,tsx,css,scss}'",
    "clear-cache": "node clear-cache.js",
    "dev:fresh": "npm run clear-cache && npm start",
    "reinstall": "rm -rf node_modules && npm install",
    "update-stats": "cd .. && ./scripts/update_code_stats.sh",
    "prestart": "npm run update-stats",
    "prebuild": "npm run update-stats"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.13.5",
    "@types/styled-components": "^5.1.34",
    "@types/three": "^0.158.3",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "http-server": "^14.1.1",
    "nodemon": "^3.0.1",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.5",
    "serve": "^14.2.1",
    "source-map-explorer": "^2.5.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "watch": "^1.0.2"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  }
}
